<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXToggleButton?>
<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.ComboBox?>
<AnchorPane fx:id="bigPane" prefHeight="850.0" prefWidth="1250.0" stylesheets="@app.css" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Map">
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="850.0" prefWidth="146.0" HBox.hgrow="SOMETIMES">
               <children>
                  <fx:include fx:id="Buttons" source="partialButtons.fxml" />
                  <JFXButton fx:id="backBtn" onAction="#backBtnClick" text="Back">
                     <styleClass>
                        <String fx:value="btn" />
                        <String fx:value="btn-secondary" />
                        <String fx:value="btn-sm" />
                     </styleClass>
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin>
                  </JFXButton>
                  <JFXToggleButton fx:id="dancePartyBtn" onAction="#danceParty" text="Dance party?" visible="false">
                     <VBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </VBox.margin></JFXToggleButton>
                  <ComboBox fx:id="floorsMenu" prefWidth="150.0" />
                  <Button fx:id="addNodeButton" onAction="#addNodeButtonClick" mnemonicParsing="false" prefHeight="31.0" prefWidth="109.0" text="Add Node">
                     <VBox.margin>
                        <Insets bottom="20.0" top="100.0" />
                     </VBox.margin></Button>
                  <Button fx:id="addPathButton" onAction="#addPathButtonClick" mnemonicParsing="false" prefHeight="31.0" prefWidth="119.0" text="Add Path" />
                  <Button fx:id="editNodeButton" onAction="#editNodeButtonClick" mnemonicParsing="false" text="Edit Details">
                     <VBox.margin>
                        <Insets bottom="20.0" top="20.0" />
                     </VBox.margin></Button>
                  <Button fx:id="deleteNodeButton" onAction="#deleteNodeButtonClick" mnemonicParsing="false" prefHeight="31.0" prefWidth="115.0" text="Del Node" />
                  <Button fx:id="deletePathButton" onAction="#deletePathButtonClick" mnemonicParsing="false" prefHeight="31.0" prefWidth="115.0" text="Del Path">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
            <VBox HBox.hgrow="SOMETIMES">
               <children>
                  <Pane fx:id="imInPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                     <children>
                        <ImageView fx:id="mapImg" fitHeight="850.0" fitWidth="1250.0" pickOnBounds="true" preserveRatio="true" />
                     </children>
                  </Pane>
               </children>
            </VBox>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="SOMETIMES" />
         </children>
      </HBox>
   </children>
</AnchorPane>
