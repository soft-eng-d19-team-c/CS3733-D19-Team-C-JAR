   <children>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <JFXButton fx:id="orderButton" alignment="TOP_LEFT" onAction="#orderAction" text="Order Prescription">
               <padding>
                  <Insets bottom="20.0" />
               </padding>
            </JFXButton>
            <JFXButton fx:id="provideButton" onAction="#provideAction" text="Provide Prescription" />
         </children>
      </VBox>
   </children>
</AnchorPane>
